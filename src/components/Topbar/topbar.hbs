<div class='topbar'>
    <span class='topbar__search'></span>
    <span class='topbar-icons'>
        <img alt class='topbar-icon' src='/static/img/notifications-none.svg' />
        <img
            alt
            id='settings'
            class='topbar-icon'
            src='/static/img/settings.svg'
        />
    </span>
    {{#if authenticated}}
        <img alt class='topbar-profile' src='/static/img/{{avatar}}' />
        <img
            alt
            class='topbar-auth'
            data-action='logout'
            src='/static/img/logout.png'
        />
    {{else}}
        <img
            alt
            class='topbar-auth'
            data-link
            href='/signin'
            src='/static/img/login.png'
        />
    {{/if}}
</div>
