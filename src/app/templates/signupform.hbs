<form class="auth-form" id="auth-form" action="#">
    {{# each inputs}}
        {{# render this}}{{/render}}
    {{/each}}
    <div class="auth-form__fail_msg">
        {{ fail_msg }}
    </div>
    <div class="auth-form__buttons">
        <input class="auth-form__submit" type="image" src="/src/static/img/signup.svg" />{{ button_signup_msg }}</button>
        <div class="auth-form__redirect">
            <p>{{ p_redirect_msg }}</p>
            <a data-link href="/signin" class="redirect-msg">{{ a_redirect_msg }}</a>
        </div>
    </div>
</form>
