<div class="topbar">
    <span class="topbar__search"></span>
    <span class="topbar-icons">
            <img class="topbar-icon" src="/src/static/img/notifications-none.svg">
            <img id="settings" class="topbar-icon" src="/src/static/img/settings.svg">
    </span>
    {{# if authenticated }}
        <img class="topbar-auth" data-action="logout" src="/src/static/img/{{ avatar }}">
    {{ else }}
        <img class="topbar-auth" data-link href="/signin" src="/src/static/img/login.png">
    {{/if}}
</div>
